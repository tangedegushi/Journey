<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:zzq="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_main4_history"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/main_bg"
    tools:context="com.rollup.journey.activity.Main4HistoryActivity">

    <com.rollup.journey.customview.TitleView
        android:id="@+id/main4_history_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        zzq:text_title="@string/str_main_other_history"
        zzq:img_right="@mipmap/save_team_record">
    </com.rollup.journey.customview.TitleView>

    <ListView android:id="@+id/main4_other_history_listView"
        android:layout_below="@id/main4_history_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        android:layout_marginTop="@dimen/layout_margin_top">
    </ListView>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true">
        <include layout="@layout/bottom_item_state"/>
    </LinearLayout>

</RelativeLayout>
